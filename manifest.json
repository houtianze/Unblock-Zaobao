{
  "name": "Unblock Zaobao",
  "version": "0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "18.0",
  "description": "Try to bypass the oversea limitation of zaobao.com",
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.zaobao.com/*",
    "*://*.zaobao.com.sg/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "short_name": "UnblkZaobao"
}
